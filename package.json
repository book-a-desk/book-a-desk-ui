{
  "name": "book-a-desk-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:ci": "vue-cli-service test:unit --ci --testResultsProcessor='jest-junit'",
    "test:unit:w": "vue-cli-service test:unit --watch",
    "test:unit:c": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "dependencies": {
    "@okta/okta-auth-js": "^7.2.0",
    "@okta/okta-signin-widget": "^7.4.0",
    "@okta/okta-vue": "^5.5.0",
    "@socheatsok78/storybook-addon-vuetify": "^0.1.9",
    "@testing-library/vue": "^6.6.1",
    "axios": "^0.21.4",
    "core-js": "^3.6.5",
    "flush-promises": "^1.0.2",
    "jest-junit": "^12.2.0",
    "moment": "^2.29.1",
    "vue": "^3.2.47",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@mdi/font": "^5.8.55",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/vue3": "^6.5.16",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "chromatic": "^6.17.1",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "^2.7.14",
    "webpack-plugin-vuetify": "^2.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
